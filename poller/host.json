{
    "version": "2.0",
    "extensionBundle": {
    	"id": "Microsoft.Azure.Functions.ExtensionBundle",
	"version": "[3.*, 4.0.0)"
    },
    "logging": {
        "logLevel": {
        "default": "Information",
        "Function": "Trace",
        "Host.Results": "Error",
        "Host.Aggregator": "Trace"
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    }
}